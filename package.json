{
  "name": "modern-js-stack",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "yarn dev:start",
    "dev:start": "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node src/server",
    "dev:wds": "webpack-dev-server --progress --mode production",
    "prod:build": "rimraf lib dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack",
    "prod:start": "heroku local",
    "heroku-postbuild": "yarn prod:build",
    "lint": "eslint src webpack.config.babel.js --ext .js,.jsx",
    "test": "yarn lint && flow && jest --coverage"
  },
  "engines": {
    "node": "16.x",
    "yarn": "1.22.x"
  },
  "browserslist": [
    "> 1%"
  ],
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/plugin-transform-react-jsx": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-flow": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-flow-react-proptypes": "^26.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.1.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-compat": "^3.13.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4 || ^3 || ^2.3.0 || ^1.7.0",
    "fetch-mock": "^9.11.0",
    "flow-bin": "^0.163.0",
    "husky": "^7.0.0",
    "jest": "^27.3.1",
    "nodemon": "^2.0.14",
    "redux-mock-store": "^1.5.4",
    "rimraf": "^3.0.2",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@popperjs/core": "^2.10.2",
    "bootstrap": "4",
    "codecov": "^3.8.3",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "immutable": "^4.0.0",
    "isomorphic-fetch": "^3.0.0",
    "jquery": "^3.6.0",
    "popper": "^1.0.1",
    "popper.js": "^1.16.1",
    "postinstall": "^0.7.4",
    "process": "^0.11.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hot-loader": "^4.5.3",
    "react-jss": "^10.8.2",
    "react-redux": "^7.2.6",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.2",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.4.0",
    "socket.io": "^4.3.2",
